@model TermProject.Models.User
@{
    
}

<br /><br /><br /><br />
<h1>Your Profile</h1>





<div>
    <form action="/Home/UpdateProfile" class="form-horizontal" role="form">

        <input type="hidden" name="Id" value="@Model.Id" />
        <div class="form-group">
            <label class="control-label col-lg-3" for="name">Name: </label>
            <div class="col-lg-4">
                <input type="text" class="form-control" name="Name" readonly value="@Model.Name"/>
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-lg-3" for="email">Email:</label>
            <div class="col-lg-4">
                <input type="email" class="form-control" name="Email" readonly value="@Model.Email" />
            </div>
        </div>


        <div class="form-group">
            <label class="control-label col-lg-3" for="address">Address:</label>
            <div class="col-lg-4">
                <input type="text" class="form-control" name="Address" readonly value="@Model.Address" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-lg-3" for="country">Phone: </label>
            <div class="col-lg-4">
                <input type="number" class="form-control" name="Phone" readonly value="@Model.Phone" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-lg-3" for="country">CNIC: </label>
            <div class="col-lg-4">
                <input type="number" class="form-control" name="Cnic" readonly value="@Model.Cnic" />
            </div>
        </div>
        <input type="hidden" name="Bolck" value="NOT" />

        <div class="form-group">
            <label class="control-label col-lg-3" for="pwd">Password:</label>
            <div class="col-lg-4">
                <input type="text" class="form-control" name="Password" readonly value="@Model.Password" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-offset-3 col-lg-10">
                <input type="submit" value="Update" class="btn btn-default" />
            </div>
        </div>

    </form>
</div>


